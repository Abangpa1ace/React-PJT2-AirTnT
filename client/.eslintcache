archOn, setSearchOn] = useState(true);\n\n  // Search Values\n  const [searchValue, setSearchValue] = useState({\n    location: '',\n    dateIn: null,\n    dateOut: null,\n    dateDiff: 0,\n    guest: 0,\n  });\n\n  const [filterValue, setFilterValue] = useState({\n    type: [],\n    price: [10000, 1000000],\n    facilities: {\n      bed: 0,\n      bedroom: 0,\n      bathroom: 0,\n    }\n  })\n\n  // Rests Data\n  const [restsTotal, setRestsTotal] = useState(0);\n  const [restList, setRestList] = useState([]);\n  const [page, setPage] = useState(1);\n  const LIMIT = 15;\n\n  const fetchRests = async () => {\n    const { type, price, facilities } = filterValue\n    const response = await fetch(\n      `${RestsAPI}?search[location]=${searchValue.location}&&filter[type]=${type}&filter[price]=${price}&page=${page}&limit=${LIMIT}\n      `, \n      { method: 'GET' }\n    );\n    const result = await response.json();\n    setRestList(result.restsList);\n    setRestsTotal(result.restsTotal);\n  }\n  \n  useEffect(() => {\n    fetchRests();\n  }, [page]);\n\n  const handleNavFixed = () => {\n    const { pageYOffset } = window;\n    let isFixed = pageYOffset >= 60;\n    setNavFixed(isFixed);\n    setSearchOn(!isFixed);\n  }\n\n  const updateSearchValue = (name, value) => {\n    setSearchValue({\n      ...searchValue,\n      [name]: value,\n    })\n  };\n\n  const updateFilterValue = (name, value) => {\n    setFilterValue({\n      ...filterValue,\n      [name]: value,\n    })\n  }\n\n  return (\n    <AppContext.Provider\n      value={{\n        navFixed,\n        setNavFixed,\n        handleNavFixed,\n        searchOn,\n        setSearchOn,\n        fetchRests,\n        searchValue,\n        setSearchValue,\n        updateSearchValue,\n        filterValue,\n        setFilterValue,\n        updateFilterValue,\n        restList,\n        setRestList,\n        restsTotal,\n        page,\n        setPage,\n        LIMIT,\n      }}>\n      {children}\n    </AppContext.Provider>\n  );\n};\n\nexport const useGlobalContext = () => {\n  const context = useContext(AppContext);\n  if (!context) {\n    throw new Error('Context is not finded.');\n  }\n  return context;\n} \n\nexport { AppContext, AppProvider };","/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/components/Global/GlobalComponent.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/components/Footer/components/FooterLinks.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/components/Footer/components/FooterRights.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/components/Footer/components/RightsLeft.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/components/Footer/FooterData.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/components/Footer/components/RightsRight.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/data/config.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/pages/List/List.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/pages/Home/components/HomeHosting.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/pages/Home/components/HomeImage.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/pages/Home/components/HomeRecommend.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/pages/Home/components/HomeCities.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/pages/Home/components/HomeExperience.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/pages/Home/HomeData.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/Components/Footer/Footer.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/Components/Footer/components/FooterRights.js",[],"/Users/taeng9/Desktop/project/React/react-pjt2-airbnb/client/src/Components/Footer/compon